`timescale 1ps/1ps
module cmp1_TB();
	reg [7:0]PI;
	reg ctrl = 1'b0;
	wire [7:0]PO;

	cmp1 C(PO,PI,ctrl);

	always #10 clk = ~clk;

	initial begin
	#10 rst = 1'b1;
	#20 rst = 1'b0;
	#20 Load = 1'b1;
	PI = $random();
	#20 Load = 1'b0;
	Shift = 1'b1;
	SerIn = $random();
	#40 $stop;
	end
endmodule